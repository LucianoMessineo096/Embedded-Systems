		;INIZIALIZZAZIONE
		
		MOV		R0,#2
		MOV		R1,#33
		
		;SOTTO ABBIAMO IL CODICE RELATIVO AL CICLO CHE IMPLEMENTA IL PRODOTTO TRA INTERI
		;IL PRODOTTO PUO' ESSERE PENSATO COME LA SOMMA PER 33 VOLTE DEL VALORE 2 , OVVERO
		;SOMMA = 2+2+2+2+....+2
		;DUNQUE R1 MI SERVE COME VARIABILE CONTATORE ,ED OGNI VOLTA CHE EFFETTUO IL CICLO LA DECREMENTO FINO A CHE NON DIVENTI NULLA
		;NEL CASO IN CUI NON FOSSE NULLA SALTO ALLA RIGA 13 , ALTRIMENTI VADO ALLA RIGA 11 E TERMINO IL PROGRAMMA
				
		CICLO	CMP		R1,#0 
				BEQ		FINE
				SUB		R1,R1,#1
				ADD		R0,R0,#2
				B		CICLO
				
		FINE		END
