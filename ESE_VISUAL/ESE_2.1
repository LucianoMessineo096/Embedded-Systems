		;Write	an assembly program that loads 2 in register r0 and then executes r0:=r0*33
		
START
		
		
		;INIZIALIZZO
		
		MOV		R0,#2
		
		;ALLORA	POSSO VEDERE IL PRODOTTO COME UNO SHIFT LOGICO A SINISTRA
		;IN		TAL CASO R0=R0*33=R0*(32+1)=R0*32+R0 , QUESTO PERCHE' I REGISTRI ARM SONO A 32 BIT DUNQUE BASTA EFFETTUARE LO SHIFT A 32 BIT , OVVERO 2^5=32
		;CIOE'	SHIFTO A SINISTRA DI 5 POSTI, POI BASTA SOMMARE L'ADDENDO R0 CHE RIMANE
		
		;SALVO	IL VALORE RIMANENTE IN UN ALTRO REGISTRO
		
		MOV		R1,R0
		
		;EFFETTUO	LO SHIFT
		LSL		R0,R0,#5
		
		;SOMMO	IL VALORE RIMANENTE
		ADD		R0,R0,R1
		
		
		END
